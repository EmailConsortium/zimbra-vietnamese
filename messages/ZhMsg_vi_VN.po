#: mail.NO_SUCH_UPLOAD
msgid "Specified upload does not exist."
msgstr "Tập tin tải lên đã chỉ định không tồn tại."

#: mail.SCAN_ERROR
msgid "Upload could not be scanned."
msgstr "Tập tin tải lên không thể quét."

#: mail.UPLOAD_REJECTED
msgid "Upload rejected."
msgstr "Tập tin tải lên bị từ chối."

#: mail.TOO_MANY_TAGS
msgid "A tag could not be created because you have exceeded the limit."
msgstr "Một thẻ không thể được tạo vì bạn đã vượt qua giới hạn."

#: mail.TOO_MANY_UPLOADS
msgid "Too many uploads."
msgstr "Quá nhiều tập tin tải lên."

#: mail.TOO_MANY_CONTACTS
msgid "A contact could not be created because you exceeded your quota."
msgstr "Một liên hệ không thể được tạo ra bởi vì bạn đã vượt quá hạn ngạch."

#: mail.UNABLE_TO_IMPORT_CONTACTS
msgid "Unable to import contacts."
msgstr "Không thể nhập các liên hệ."

#: mail.QUOTA_EXCEEDED
msgid ""
"Your message could not be sent because you have exceeded your mail quota."
msgstr ""
"Thông điệp của bạn có thể không được gửi đi bởi vì bạn đã vượt quá hạn ngạch "
"thư của bạn."

#: mail.INVALID_ID
msgid "Invalid id."
msgstr "Id không hợp lệ."

#: mail.INVALID_NAME
msgid "Invalid name. It contains at least one invalid character."
msgstr "Tên không hợp lệ. Nó chứa ít nhất một ký tự không hợp lệ."

#: mail.INVALID_TYPE
msgid "Invalid type."
msgstr "Kiểu không hợp lệ"

#: mail.INVALID_CONTENT_TYPE
msgid "Invalid contact type."
msgstr "Loại liên hệ không hợp lệ."

#: mail.WRONG_MAILBOX
msgid "Cannot combine objects from different mailboxes."
msgstr "Không thể kết hợp các đối tượng từ hộp thư khác nhau."

#: mail.CANNOT_SUBSCRIBE
msgid "Cannot add subscription to existing folder."
msgstr "Không thể thêm đăng ký tới thư mục đang tồn tại."

#: mail.CANNOT_CONTAIN
msgid "Cannot put object in that folder."
msgstr "Không thể đặt đối tượng trong thư mục đó."

#: mail.CANNOT_COPY
msgid "Cannot copy object."
msgstr "Không thể sao chép đối tượng."

#: mail.CANNOT_TAG
msgid "Cannot tag that item."
msgstr "Không thể gắn thẻ mục đó."

#: mail.CANNOT_PARENT
msgid "Cannot make object a child of that parent."
msgstr "Không thể làm một đối tượng lcaf con của cha nó."

#: mail.IS_NOT_CHILD
msgid "Tried to remove object from non-parent."
msgstr "Cố gắng loại bỏ đối tượng từ đôi tượng không phả là cha."

#: mail.MODIFY_CONFLICT
msgid ""
"Item version number conflict. Another version was added between edit and "
"save."
msgstr ""
"Số phiên bản của mục xung đột. Phiên bản khác đã được thêm vào giữa lúc sửa "
"và thêm."

#: mail.IMMUTABLE_OBJECT
msgid "Cannot modify immutable object."
msgstr "Không thể sửa đổi các đối tượng bất biến."

#: mail.ALREADY_EXISTS
msgid "Item already exists."
msgstr "Mục đã tồn tại."

#: mail.QUERY_PARSE_ERROR
msgid "Unable to parse the search query."
msgstr "Không thể phân tích truy vấn tìm kiếm."

#: mail.MESSAGE_PARSE_ERROR
msgid "Unable to parse message."
msgstr "hông thể phân tích thông điệp."

#: mail.ADDRESS_PARSE_ERROR
msgid "Unable to parse address."
msgstr "hông thể phân tích địa chỉ."

#: mail.ICALENDAR_PARSE_ERROR
msgid "Unable to parse iCalendar."
msgstr "hông thể phân tích iCalendar."

#: mail.MUST_BE_ORGANIZER
msgid "Must be the appointment organizer."
msgstr "Phải là người tổ chức cuộc hẹn."

#: mail.CANNOT_CANCEL_INSTANCE_OF_EXCEPTION
msgid "Cannot cancel instance of exception."
msgstr "Không thể huỷ bỏ mẫu của ngoại lệ."

#: mail.INVITE_OUT_OF_DATE
msgid "Invite is out of date."
msgstr "Lời mời đã quá ngày."

#: mail.SEND_ABORTED_ADDRESS_FAILURE
msgid "Could not send message due to invalid address(es)"
msgstr "Không thể gửi thông điệp do địa chỉ không hợp lệ"

#: mail.SEND_PARTIAL_ADDRESS_FAILURE
msgid "Partial address failure on send."
msgstr "Một phần địa chỉ lỗi khi gửi."

#: mail.SEND_FAILURE
msgid "Could not send message."
msgstr "Không thể gửi thông điệp."

#: mail.TRY_AGAIN
msgid "Try again."
msgstr "Hãy thử lại."

#: mail.TOO_MANY_QUERY_TERMS_EXPANDED
msgid "Too many query terms expanded."
msgstr "Quá nhiều điều khoản yêu cầu tìm kiếm mở rộng."

#: mail.MESSAGE_TOO_BIG
msgid ""
"This file cannot be attached because it has exceeded the maximum allowed "
"size."
msgstr ""
"Tập tin này không thể được đính kèm bởi vì nó đã vượt quá kích thước tối đa "
"cho phép."

#: zclient.CLIENT_ERROR
msgid "A network service error has occurred (zclient)."
msgstr "Một lỗi dịch vụ mạng vừa xảy ra (zclient)."

#: zclient.IO_ERROR
msgid "A network service error has occurred (zclient/io)."
msgstr "Một lỗi dịch vụ mạng vừa xảy ra (zclient/io)."

#: zclient.UPLOAD_SIZE_LIMIT_EXCEEDED
msgctxt "zclient.UPLOAD_SIZE_LIMIT_EXCEEDED"
msgid "Attachment size limit exceeded."
msgstr "Kích thước tập tin đính kèm vượt quá giới hạn."

#: zclient.UPLOAD_FAILED
msgctxt "zclient.UPLOAD_FAILED"
msgid "Attachment upload failed."
msgstr "Tải lên tập tin đính kèm không thành công."

#: ztaglib.TAG_EXCEPTION
msgid "A client error occurred. Please try again."
msgstr "Một lỗi trình khách đã xảy ra. Hãy thử lại."

#: ztaglib.UPLOAD_SIZE_LIMIT_EXCEEDED
msgctxt "ztaglib.UPLOAD_SIZE_LIMIT_EXCEEDED"
msgid "Attachment size limit exceeded."
msgstr "Kích thước tập tin đính kèm vượt quá giới hạn."

#: ztaglib.UPLOAD_FAILED
msgctxt "ztaglib.UPLOAD_FAILED"
msgid "Attachment upload failed."
msgstr "Tải lên tập tin đính kèm không thành công."

#: ztaglib.EMPTY_CONTACT
msgid "Contact must have at least one field set."
msgstr "Liên hệ phải có ít nhất một tập hợp các trường."

#: ztaglib.INVALID_FILTER_DATE
msgid "Invalid filter date format."
msgstr "Định dạng ngày của bộ lọc không hợp lệ."

#: ztaglib.INVALID_APPT_DATE
msgid "Invalid appointment date format."
msgstr "Định dạng ngày của cuộc hẹn không hợp lệ."

#: ztaglib.FILTER_EXISTS
msgid "A filter with that name already exists."
msgstr "Một bộ lọc có tên này đã tồn tại."

#: ztaglib.NO_SUCH_FILTER_EXISTS
msgid "No filter with that name exists."
msgstr "Không có bộ lọc có tên này tồn tại."

#: ztaglib.INVALID_CRUMB
msgid "Action not allowed without a valid crumb."
msgstr "Hành động không được phép mà không có một crumb hợp lệ."

