#: errorEmptyResponse
msgid "Received empty response from server"
msgstr "Nhận phản hồi trống từ máy chủ"

#: errorMaintenanceMode
msgid "The account is in maintenance mode"
msgstr "Tài khoản đang trong chế độ bảo trì"

#: BAD_JSON_RESPONSE
msgctxt "BAD_JSON_RESPONSE"
msgid "A network error has occurred."
msgstr "Một lỗi mạng đã được xảy ra."

#: EMPTY_RESPONSE
msgid "Cannot connect to server."
msgstr "Không thể kết nối tới máy chủ."

#: NETWORK_ERROR
msgctxt "NETWORK_ERROR"
msgid "A network error has occurred."
msgstr "Một lỗi mạng vừa xảy ra."

#: SOAP_ERROR
msgctxt "SOAP_ERROR"
msgid "A network error has occurred."
msgstr "Một lỗi mạng vừa xảy ra."

#: CSFE_SVC_ERROR
msgctxt "CSFE_SVC_ERROR"
msgid "A network service error has occurred."
msgstr "Một lỗi dịch vụ mạng vừa xảy ra."

#: account.AUTH_FAILED
msgid ""
"The username or password is incorrect. Verify that CAPS LOCK is not on, and "
"then retype the current username and password."
msgstr ""
"Tên người dùng và mật khẩu không đúng. Kiểm tra xem CAPS LOCK là không bật, "
"và sau đó gõ lại tên người dùng và mật khẩu hiện thời."

#: account.CHANGE_PASSWORD
msgid "Your password is no longer valid. Please choose a new password."
msgstr "Mật khẩu của bạn không còn hợp lệ nữa. Vui lòng chọn một mật khẩu mới."

#: account.INVALID_PASSWORD
msgid "You have entered an invalid password."
msgstr "Bạn vừa nhập một mật khẩu không hợp lệ."

#: account.INVALID_PREF_NAME
msgid "Invalid preference name."
msgstr "Tên tham chiếu không hợp lệ."

#: account.INVALID_PREF_VALUE
msgid "Invalid preference value."
msgstr "Giá trị tham chiếu không hợp lệ."

#: account.MAINTENANCE_MODE
msgctxt "account.MAINTENANCE_MODE"
msgid "This account is currently in maintenance mode."
msgstr "Tài khoản này hiện thời đang trong chế độ bảo trì."

#: account.NO_SUCH_ACCOUNT
msgid "No such account exists."
msgstr "Không có tài khoản nào như vậy tồn tại."

#: account.NO_SUCH_SAVED_SEARCH
msgid "No such saved search exists."
msgstr "Không việc tìm kiếm được ghi như vậy tồn tại."

#: account.ACCT_NO_SUCH_TAG
msgctxt "account.ACCT_NO_SUCH_TAG"
msgid "No such tag exists."
msgstr "Không thẻ nào như vậy tồn tại."

#: account.TOO_MANY_IDENTITIES
msgid ""
"The identity could not be created because you have exceeded your identity "
"quota."
msgstr "Định danh không thể được tạo vì bạn đã quá giới hạn định danh."

#: account.PASSWORD_CHANGE_TOO_SOON
msgid "Password cannot be changed too soon."
msgstr "Mật khẩu không thể được thay đổi quá sớm."

#: account.PASSWORD_LOCKED
msgid ""
"Password locked. User not allowed to change password. Please contact your "
"System Administrator."
msgstr ""
"Mật khẩu đã khoá. Người dùng không được phép thay đổi mật khẩu. Vui lòng "
"liên hệ tới Quản trị hệ thống của bạn."

#: account.PASSWORD_RECENTLY_USED
msgid ""
"The password you submitted has recently been used. Please choose a different "
"password."
msgstr ""
"Mật khẩu mà bạn đã gửi vừa mới được sử dụng gần đây. Vui lòng chọn một mật "
"khẩu khác."

#. # {0} = filename
#: formatter.INVALID_FORMAT
msgid "Invalid file format for file \"{0}\"."
msgstr "Định dạng tập tin không hợp lệ đối với tập tin \"{0}\"."

#. # {0} = view, {1} = folder name
#: formatter.INVALID_TYPE
msgid "Folder \"{1}\" cannot contain items of type \"{0}\"."
msgstr "Thư mục \"{1}\" không thể chứa các mục có kiểu \"{0}\"."

#. # {0} = item name
#: formatter.MISMATCHED_META
msgid "Meta information for item \"{0}\" does not match content."
msgstr "Thông tin meta cho mục \"{0}\" không khớp với nội dung."

#: formatter.MISMATCHED_SIZE
msgid "Size of ignored item \"{0}\" does not match."
msgstr "Kích thước của mục đã bỏ \"{0}\" không khớp."

#: formatter.MISMATCHED_TYPE
msgid "Cannot overwrite item \"{0}\" of different type."
msgstr "Không thể ghi đè lên mục  \"{0}\" do khác kiểu."

#: formatter.MISSING_BLOB
msgid "Content for item \"{0}\" is missing."
msgstr "Nội dung của mục \"{0}\" đang thiếu."

#: formatter.MISSING_META
msgid "Meta information for item \"{0}\" is missing."
msgstr "Thông tin meta của mục \"{0}\" đang thiếu."

#: formatter.MISSING_VCARD_FIELDS
msgid "Contact item \"{0}\" does not contain any fields."
msgstr "Mục liên hệ \"{0}\" không chứa bất kỳ trường nào."

#. # {0} = item name, {1} = original error message
#: formatter.UNKNOWN_ERROR
msgid "Unknown error for item \"{0}\": {1}"
msgstr "Lỗi không biết của mục \"{0}\": {1}"

#. # The im.lower_case messages are received as part of a chat message
#: im.forbidden
msgid "Forbidden."
msgstr "Ngăn cấm."

#: im.internal_server_error
msgid "An internal server error occurred."
msgstr "Một lỗi trong máy chủ đã xảy ra."

#: im.item_not_found
msgid "The user was not found. The message may not have been sent."
msgstr "Người dùng không được tìm thấy. Thông điệp có thể không được gửi."

#: im.jid_malformed
msgctxt "im.jid_malformed"
msgid "The address is invalid."
msgstr "Địa chỉ không hợp lệ."

#: im.not_acceptable
msgid "The message is not acceptable."
msgstr "Thông điệp không được chấp nhận."

#: im.not_allowed
msgid "You are not allowed to send a message to the intended recipient."
msgstr "Bạn không được phép gửi một thông điệp tới người nhận được dự định."

#: im.payment_required
msgid "Payment required."
msgstr "Yêu cầu thanh toán."

#: im.recipient_unavailable
msgid "The recipient is unavailable."
msgstr "Người nhận không có."

#: im.redirect
msgid "The user is at a different address."
msgstr "Người dùng tại một địa chỉ khác."

#: im.remote_server_not_found
msgid "The remote server could not be found."
msgstr "Máy chủ ở xa không được tìm thấy."

#: im.remote_server_timeout
msgid "The connection to the remote server timed out."
msgstr "Kết nối tới máy chủ ở xa hết thời gian chờ."

#: im.resource_constraint
msgid "The system is busy."
msgstr "Hệ thống đang bận."

#: im.service_unavailable
msgid "Messages are not accepted by the service."
msgstr "Các thông điệp không được chấp nhận bởi dịch vụ."

#: im.undefined_condition
msgid "An undefined error occurred. "
msgstr "Một lỗi chưa định nghĩa đã xảy ra."

#: im.unknown_error
msgid "An unknown error occurred."
msgstr "Một lỗi không biết đã xảy ra."

#. # The following errors happen when trying to join a chat room
#: im.Banned
msgid "You have been banned from the room."
msgstr "Bạn đã bị cấm từ phòng."

#: im.MaxUsers
msgid "The room has reached capacity."
msgstr "Phòng này đầy."

#: im.MustUseReservedRoomnick
msgid "You must use a reserved nickname."
msgstr "Bạn phải sử dụng một biệt hiệu có trước."

#: im.NicknameConflict
msgid "Someone else is already using your nickname."
msgstr "Ai đó đã sử dụng biệt danh của bạn."

#: im.NoSuchRoom
msgid "No such room."
msgstr "Không có phòng như vậy."

#: im.NotAllowed
msgid "You are not allowed to create the room."
msgstr "Bạn không được phép tạo một phòng."

#: im.NotAMember
msgid "The room is invite-only, and you aren't a member."
msgstr "Phòng chỉ mời, và bạn phải là một thành viên."

#: im.PasswordRequired
msgid "A password is required to enter the room."
msgstr "Cần có một mật khẩu để vào phòng."

#: im.Unknown
msgid "Unknown error."
msgstr "Một lỗi không biết đã xảy ra."

#: im.INVALID_ADDRESS
msgctxt "im.INVALID_ADDRESS"
msgid "The address is invalid."
msgstr "Địa chỉ không hợp lệ."

#: mail.ALREADY_EXISTS
msgid "An object with that name already exists."
msgstr "Một đối tượng mà có tên đã tồn tại."

#: mail.MODIFY_CONFLICT
msgid ""
"Item version number conflict. Another version was added between edit and "
"save."
msgstr ""
"Số phiên bản xung đột. Phiên bản khác đã được thêm vào giữa để sửa và lưa "
"lại."

#: mail.INVALID_NAME
msgid ""
"Sorry, \"{0}\" is not a valid name. It contains at least one invalid "
"character."
msgstr ""
"Xin lỗi, \"{0}\" không phải là một tên hợp lệ. Nó chứa ít nhất một ký tự "
"không hợp lệ."

#: mail.INVITE_OUT_OF_DATE
msgid "The invite has been modified and is no longer current."
msgstr "Lời mời đã được sửa đổi và không còn hiện hành."

#: mail.MAINTENANCE
msgctxt "mail.MAINTENANCE"
msgid "This account is currently in maintenance mode."
msgstr "Tại khoản này hiện thời đang trong chế độ bảo trì."

#: mail.MESSAGE_TOO_BIG
msgid ""
"This message cannot be sent because it has exceeded the maximum allowed size."
msgstr ""
"Thông điệp này không thể được gửi đi vì nó kích cỡ của nó vượt qua kích cỡ "
"tối đa."

#: mail.NO_SUCH_CONV
msgid "No such conversation exists."
msgstr "Không có cuộc đàm thoại như vậy tồn tại."

#: mail.NO_SUCH_FOLDER
msgid "No such folder exists."
msgstr "Không có thư mục như vậy tồn tại."

#: mail.NO_SUCH_ITEM
msgid "Item \"{0}\" does not exist."
msgstr "Mục \"{0}\" không tồn tại."

#: mail.NO_SUCH_CALITEM
msgid ""
"Calendar or task item \"{0}\" does not exist. It may have been rescheduled."
msgstr ""
"Mục lịch và công việc \"{0}\" không tồn tại. Nó có thể đã được lên lịch lại."

#: mail.NO_SUCH_MSG
msgid "No such message exists."
msgstr "Không có thông điệp như vậy tồn tại."

#: mail.NO_SUCH_PART
msgid "No such message part exists."
msgstr "Không có phần thông điệp như vậy tồn tại."

#: mail.NO_SUCH_TAG
msgctxt "mail.NO_SUCH_TAG"
msgid "No such tag exists."
msgstr "Không có thẻ như vậy tồn tại."

#: mail.QUERY_PARSE_ERROR
msgid "Unable to parse the search query."
msgstr "Không thể phân tích chuỗi truy vấn tìm kiếm."

#: mail.QUOTA_EXCEEDED
msgid ""
"Your message could not be sent because you have exceeded your mail quota."
msgstr ""
"Thông điệp của bạn không thể được gửi vì bạn bạn đã quá hạn ngạch lưu mail."

#: mail.TOO_MANY_CONTACTS
msgid ""
"Contact could not be created because you have exceeded your contact quota."
msgstr "Liên hệ không thể được tạo bởi vì bạn đã quá hạn ngạch tạo liên hệ."

#: mail.TOO_MANY_QUERY_TERMS_EXPANDED
msgid "Please be more specific in your search criteria."
msgstr "Vui lòng chỉ định thêm trong tiêu chuẩn tìm kiếm của bạn."

#: mail.MAIL_UNABLE_TO_IMPORT_APPOINTMENTS
msgctxt "mail.MAIL_UNABLE_TO_IMPORT_APPOINTMENTS"
msgid "Unable to import. The chosen file may be invalid."
msgstr "Không thể nhập. File được chọn có thể không hợp lệ."

#: mail.MAIL_UNABLE_TO_IMPORT_CONTACTS
msgctxt "mail.MAIL_UNABLE_TO_IMPORT_CONTACTS"
msgid "Unable to import. The chosen file may be invalid."
msgstr "Không thể nhập. File được chọn có thể không hợp lệ."

#: mail.SEND_FAILURE
msgid "Could not send message: {0}"
msgstr "Không thể gửi thông điệp: {0}"

#: mail.SEND_ABORTED_ADDRESS_FAILURE
msgid "Could not send message due to invalid address(es)"
msgstr "Không thể gửi thông điệp bởi vì địa chỉ không hợp lệ"

#: mail.TOO_MANY_TAGS
msgid "A tag could not be created because you have exceeded the limit."
msgstr "Một thẻ không thể được tạo bởi vị bạn đã vượt quá giới hạn."

#: offline.ONLINE_ONLY_OP
msgid ""
"You must be online to use this feature. Please check your network connection "
"and try again."
msgstr ""
"Bạn phải trực tuyến để sử dụng tính năng này. Vui lòng kiểm tra kết nối mạng "
"của bạn và thử lại."

#: remote.TIMEOUT
msgid "A timeout error has occurred."
msgstr "Một lỗi thời gian chờ đã xảy ra."

#: service.PARSE_ERROR
msgid "A parsing error has occurred."
msgstr "Một lỗi phân tích vừa xảy ra."

#: service.PERM_DENIED
msgid "Permission denied."
msgstr "Quyền truy cập bị từ chối."

#: service.FAILURE
msgctxt "service.FAILURE"
msgid "A network service error has occurred."
msgstr "Một lỗi dịch vụ mạng vừa xảy ra."

#: service.INVALID_REQUEST
msgid "An invalid request was made."
msgstr "Một yêu cầu không hợp lệ vừa thực hiện."

#: service.UNKNOWN_DOCUMENT
msgid "Server error (unknown command sent to server)."
msgstr "Lỗi máy chủ (Lệnh không biết đã gửi tới máy chủ)."

#: service.RESOURCE_UNREACHABLE
msgid "Resource unreachable."
msgstr "Tài nguyên không lấy được."

#: service.TEMPORARILY_UNAVAILABLE
msgid "Service temporarily unavailable."
msgstr "Dịch vụ tạm thời không có."

#: service.AUTH_REQUIRED
msgid "Authentication Required"
msgstr "Yêu cầu xác thực"

#: service.AUTH_EXPIRED
msgid "Your session has expired. Please login again."
msgstr "Phiên của bạn vừa hết hạn. Vui lòng đăng nhập lại."

#: service.WRONG_HOST
msgid "Request was sent to the wrong server."
msgstr "Yêu cầu đã được gửi tới một máy chủ sai."

#: service.NON_READONLY_OPERATION_DENIED
msgid "The server is in read-only mode."
msgstr "Mãy chủ đang trong chế độ chỉ đọc."

#: service.PROXY_ERROR
msgid "Proxy error."
msgstr "Lỗi uỷ quyền."

#: service.TOO_MANY_HOPS
msgid "Too many hops between servers."
msgstr "Quá nhiều chặng giữa các máy chủ."

#: service.ALREADY_IN_PROGRESS
msgid "Operation already in progress."
msgstr "Thao tác đang thực hiện."

#: service.NOT_IN_PROGRESS
msgid "Operation not in progress."
msgstr "Thao tác đang không thực hiện."

#: service.INTERRUPTED
msgid "The operation has been interrupted"
msgstr "Thao tác đã bị ngắt"

#: service.NO_SPELL_CHECK_URL
msgid "No spell check server defined."
msgstr "Không có máy chủ kiểm tra chính tả nào được định nghĩa."

#: voice.UNABLE_TO_RETRIEVE_PROFILE_SUMMARY
msgid ""
"The system is currently unable to complete your request. We apologize for "
"this inconvenience."
msgstr ""
"Hệ thống hiện thời không thể hoàn thành yêu cầu của bạn. Chúng tôi lấy làm "
"tiếc về sự phiền phức này."

#: voice.UNABLE_TO_RETRIEVE_CALLLOGS
msgid ""
"We are currently unable to display call records. We apologize for this "
"inconvenience."
msgstr ""
"Chúng tôi hiện thời không thể hiển thị bản ghi cuộc gọi. Chúng tôi lấy làm "
"tiếc về sự phiền phức này."

#: voice.UNABLE_TO_RETRIEVE_VOICEMAIL_FOLDER
msgid "We are temporarily unable to display voice mail records."
msgstr "Chung tôi tạm thời không thể hiển thị các bản ghi thư thoại."

#: voice.UNABLE_TO_UPDATE_CALL_FEATURES
msgctxt "voice.UNABLE_TO_UPDATE_CALL_FEATURES"
msgid ""
"We are temporarily unable to forward calls. We apologize for this "
"inconvenience."
msgstr ""
"Chúng tôi tạm thời không thể chuyển tiếp cuộc gọi. Chúng tôi lấy làm tiếc về "
"sự phiền phức này."

#: voice.UNABLE_TO_MODIFY_CALL_FEATURES
msgctxt "voice.UNABLE_TO_MODIFY_CALL_FEATURES"
msgid ""
"We are temporarily unable to forward calls. We apologize for this "
"inconvenience."
msgstr ""
"Chúng tôi tạm thời không thể chuyển tiếp cuộc gọi. Chúng tôi lấy làm tiếc về "
"sự phiền phức này."

#: voice.UNABLE_TO_MODIFY_VOICEMAIL_PREFS
msgid ""
"An error occurred while attempting to update your voice mail preferences. "
"Please try again."
msgstr ""
"Một lỗi đã xay ra trong khi cố gắng cập nhật tham chiếu thư thoại của bạn. "
"Vui lòng thử lại."

#: voice.UNABLE_TO_FORWARD_VOICEMAIL
msgid "Your email could not be forwarded at this time. Please try again later."
msgstr ""
"Mail của bạn không thể được chuyển tiếp trong thời điểm hiện tại. Vui lòng "
"thử lại sau."

#: voice.UNABLE_TO_RETRIEVE_CALL_FEATURES
msgid "Unable to retrieve call features."
msgstr "Không thể các nhận đặc điểm cuộc gọi."

#: voice.UNABLE_TO_RETRIEVE_VOICEMAIL_CONTENT
msgid "Unable to retrieve voice mail content."
msgstr "Không thể nhận nội dung thư thoại."

#: voice.UNABLE_TO_RETRIEVE_VOICEMAIL_PREFS
msgid "Unable to retrieve voice mail preferences."
msgstr "Không thể nhận tùy chọn thư thoại."

#: voice.UNABLE_TO_RETRIEVE_VOICEMAIL_SUMMARY
msgid "Unable to retrieve voice mail summary."
msgstr "Không thể nhận bản tóm tắt của thư thoại."

#: voice.UNABLE_TO_MODIFY_VOICEMAIL_STATUS
msgid "Unable to modify voice mail status. "
msgstr "Không thể thay đổi trạng thái thư thoại."

#: voice.UNABLE_TO_EMPTY_VOICEMAIL_TRASH
msgid "Unable to empty voice mail trash."
msgstr "Không thể làm trống thùng rác thư thoại."

#: voice.UNABLE_TO_DELETE_VOICEMAIL
msgid "Unable to delete voice mail."
msgstr "Không thể xoá thư thoại."

#: voice.UNABLE_TO_UNDELETE_VOICEMAIL
msgid "Unable to undelete voice mail."
msgstr "Không thể khôi phục thư thoại đã xoá."

#: zclient.CLIENT_ERROR
msgid "A network service error has occurred (zclient)."
msgstr "Một lỗi dịch vụ mạng vừa xảy ra (zclient)."

#: zclient.IO_ERROR
msgid "A network service error has occurred (zclient/io)."
msgstr "Một lỗi dịch vụ mạng vừa xảy ra (zclient/io)."

#: ztaglib.TAG_EXCEPTION
msgid "A client error occurred. Please try again."
msgstr "Một lỗi viếng thăm vừa xảy ra. Vui lòng thử lại."

#: unknownError
msgid "An unknown error ({0}) has occurred."
msgstr "Một lỗi không biết ({0}) đã xảy ra."
