msgstr "Hộp thư hiện thời đang trong chế độ bảo trì."

#: mail.NO_SUCH_MBOX
msgid "Mailbox does not exist."
msgstr "Hộp thư không tồn tại."

#: mail.NO_SUCH_ITEM
msgid "No such item exists."
msgstr "Không có mục như vậy tồn tại."

#: mail.NO_SUCH_CONV
msgid "No such conversation exists."
msgstr "Không có cuộc đàm luộn như vậy tồn tại."

#: mail.NO_SUCH_MSG
msgid "No such message exists."
msgstr "Không có thông điệp như vậy tồn tại."

#: mail.NO_SUCH_PART
msgid "No such part exists."
msgstr "Không có phần như vậy tồn tại."

#: mail.NO_SUCH_CONTACT
msgid "No such contact exists."
msgstr "Không có liên hệ như vậy tồn tại."

#: mail.NO_SUCH_FOLDER
msgid "No such folder exists."
msgstr "Không có thư mục như vậy tồn tại."

#: mail.NO_SUCH_NOTE
msgid "No such note exists."
msgstr "Không có ghi chú như vậy tồn tại."

#: mail.NO_SUCH_CALITEM
msgid "No such calendar item exists."
msgstr "Không có mục lịch làm việc như vậy tồn tại."

#: mail.NO_SUCH_APPT
msgid "No such appointment exists."
msgstr "Không có cuộc hẹn như vậy tồn tại."

#: mail.NO_SUCH_TASK
msgid "No such task exists."
msgstr "Không có tác vụ như vậy tồn tại."

#: mail.NO_SUCH_DOC
msgid "No such document exists."
msgstr "Không có tài liệu như vậy tồn tại."

#: mail.NO_SUCH_TAG
msgid "No such tag exists."
msgstr "Không có thẻ như vậy tồn tại."

#: mail.NO_SUCH_UPLOAD
msgid "Specified upload does not exist."
msgstr "Tải lên được chỉ định không tồn tại."

#: mail.SCAN_ERROR
msgid "Upload could not be scanned."
msgstr "Tải lên không thể quét được."

#: mail.UPLOAD_REJECTED
msgid "Upload rejected."
msgstr "Tải lên bị từ chối"

#: mail.TOO_MANY_TAGS
msgid "A tag could not be created because you have exceeded the limit."
msgstr "Một thẻ không thể được tạo bởi vì bản đã vượt quá giới hạn."

#: mail.TOO_MANY_UPLOADS
msgid "Too many uploads."
msgstr "Quá nhiều tải lên."

#: mail.TOO_MANY_CONTACTS
msgid "A contact could not be created because you exceeded your quota."
msgstr "Một liên hệ không thể được tạo bởi vì bạn đã hết hạn ngạch"

#: mail.UNABLE_TO_IMPORT_CONTACTS
msgid "Unable to import contacts."
msgstr "Không thể nhập liên hệ."

#: mail.QUOTA_EXCEEDED
msgid ""
"Your message could not be sent because you have exceeded your mail quota."
msgstr "Thông điệp của bạn không được gửi vì bạn đã hết hạn ngạch thư của bạn."

#: mail.INVALID_ID
msgid "Invalid id."
msgstr "Id không hợp lệ."

#: mail.INVALID_NAME
msgid "Invalid name. It contains at least one invalid character."
msgstr "Tên không hợp lệ. Nó chứa ít nhất một ký tự không hợp lệ."

#: mail.INVALID_TYPE
msgid "Invalid type."
msgstr "Kiểu không hợp lệ."

#: mail.INVALID_CONTENT_TYPE
msgid "Invalid contact type."
msgstr "Kiểm liên hệ không hợp lệ."

#: mail.WRONG_MAILBOX
msgid "Cannot combine objects from different mailboxes."
msgstr "Không thể kết hợp các đối tượng từ các hộp thư khác nhau."

#: mail.CANNOT_SUBSCRIBE
msgid "Cannot add subscription to existing folder."
msgstr "Không thể thêm đăng ký vào thư mục hiện có."

#: mail.CANNOT_CONTAIN
msgid "Cannot put object in that folder."
msgstr "Không thể đặt đối tượng trong thư mục đó."

#: mail.CANNOT_COPY
msgid "Cannot copy object."
msgstr "Không thể sao chép đối tượng."

#: mail.CANNOT_TAG
msgid "Cannot tag that item."
msgstr "Không thể gắn thẻ mục đó."

#: mail.CANNOT_PARENT
msgid "Cannot make object a child of that parent."
msgstr "Không thể làm đối tượng làm con của đối tượng cha đó."

#: mail.IS_NOT_CHILD
msgid "Tried to remove object from non-parent."
msgstr "Không thể chuyển đối tượng vì không có cha."

#: mail.MODIFY_CONFLICT
msgid ""
"Item version number conflict. Another version was added between edit and "
"save."
msgstr ""
"Số phiên bản của mục xung đột. Phiên bản khác đã được thêm giữa lúc sửa và "
"lưu."

#: mail.IMMUTABLE_OBJECT
msgid "Cannot modify immutable object."
msgstr "Không thể thay đổi đối tượng không đổi"

#: mail.ALREADY_EXISTS
msgid "Item already exists."
msgstr "Mục đã tồn tại."

#: mail.QUERY_PARSE_ERROR
msgid "Unable to parse the search query."
msgstr "Không thể phân tích truy vấn tìm kiếm."

#: mail.MESSAGE_PARSE_ERROR
msgid "Unable to parse message."
msgstr "Không thể phân tích thông điệp."

#: mail.ADDRESS_PARSE_ERROR
msgid "Unable to parse address."
msgstr "Không thể phân tích địa chỉ."

#: mail.ICALENDAR_PARSE_ERROR
msgid "Unable to parse iCalendar."
msgstr "Không thể phân tích iCalendar."

#: mail.MUST_BE_ORGANIZER
msgid "Must be the appointment organizer."
msgstr "Phải là người tổ chức cuộc hẹn"

#: mail.CANNOT_CANCEL_INSTANCE_OF_EXCEPTION
msgid "Cannot cancel instance of exception."
msgstr "Không thể hủy bỏ mẫu ngoại lệ."

#: mail.INVITE_OUT_OF_DATE
msgid "Invite is out of date."
msgstr "Mới quá hạn"

#: mail.SEND_ABORTED_ADDRESS_FAILURE
msgid "Could not send message due to invalid address(es)"
msgstr "Không thể gửi thông điệp do địa chỉ không hợp lệ"

#: mail.SEND_PARTIAL_ADDRESS_FAILURE
msgid "Partial address failure on send."
msgstr "Phần địa chỉ thất bại khi gửi."

#: mail.SEND_FAILURE
msgid "Could not send message."
msgstr "Không thể gửi thông điệp."

#: mail.TRY_AGAIN
msgid "Try again."
msgstr "Thử lại."

#: mail.TOO_MANY_QUERY_TERMS_EXPANDED
msgid "Too many query terms expanded."
msgstr "Quá nhiều điều kiện truy vấn mở rộng."

#: zclient.CLIENT_ERROR
msgid "A network service error has occurred (zclient)."
msgstr "Mỗi lỗi dịch vụ mạng vừa xảy ra (zclient)."

#: zclient.IO_ERROR
msgid "A network service error has occurred (zclient/io)."
msgstr "Mỗi lỗi dịch vụ mạng vừa xảy ra (zclient/io)."

#: zclient.UPLOAD_SIZE_LIMIT_EXCEEDED
msgctxt "zclient.UPLOAD_SIZE_LIMIT_EXCEEDED"
msgid "Attachment size limit exceeded."
msgstr "Kích thước tập tin đính kèm quá giới hạn."

#: zclient.UPLOAD_FAILED
msgctxt "zclient.UPLOAD_FAILED"
msgid "Attachment upload failed."
msgstr "Tải lên tập tin đính kèm thất bại."

#: ztaglib.TAG_EXCEPTION
msgid "A client error occurred. Please try again."
msgstr "Một lỗi ở trình khách vừa xảy ra. Hãy thử lại."

#: ztaglib.UPLOAD_SIZE_LIMIT_EXCEEDED
msgctxt "ztaglib.UPLOAD_SIZE_LIMIT_EXCEEDED"
msgid "Attachment size limit exceeded."
msgstr "Kích thước tập tin đính kèm quá giới hạn."

#: ztaglib.UPLOAD_FAILED
msgctxt "ztaglib.UPLOAD_FAILED"
msgid "Attachment upload failed."
msgstr "Tải lên tập tin đính kèm thất bại."

#: ztaglib.EMPTY_CONTACT
msgid "Contact must have at least one field set."
msgstr "Liên hệ phải ít nhất thiết lập một trường."

#: ztaglib.INVALID_FILTER_DATE
msgid "Invalid filter date format."
msgstr "Định dạng ngày bộ lọc không hợp lệ."

#: ztaglib.INVALID_APPT_DATE
msgid "Invalid appointment date format."
msgstr "Định dạng ngày cuộc hẹn không hợp lệ."

#: ztaglib.FILTER_EXISTS
msgid "A filter with that name already exists."
msgstr "Một bộ lọc với tên đó đã tồn tại."

#: ztaglib.NO_SUCH_FILTER_EXISTS
msgid "No filter with that name exists."
msgstr "Không có bộ lọc với tên đó tồn tại."

#: ztaglib.INVALID_CRUMB
msgid "Action not allowed without a valid crumb."
msgstr "Hành động không được phép không có một mẩu (crumb) hợp lệ."

#. # this one should never get displayed since we should catch and redirect
#: ztaglib.SERVER_REDIRECT
msgid "Redirect to correct server."
msgstr "Chuyển hướng tới máy chủ đúng."

#: voice.UNABLE_TO_RETRIEVE_PROFILE_SUMMARY
msgid ""
"The system is currently unable to complete your request. We apologize for "
"this inconvenience."
msgstr ""
"Hệ thống hiện thời không thể hoàn thành yêu cầu của bạn. Chúng tôi xin lỗi "
"vì sự bất tiện này."

#: voice.UNABLE_TO_RETRIEVE_CALLLOGS
msgid ""
"We are currently unable to display call records. We apologize for this "
"inconvenience."
msgstr ""
"Chúng tôi tạm thời không thể hiển thị các bản ghi cuộc gọi. Chúng tôi xin "
"lỗi vì sự bất tiện này."

#: voice.UNABLE_TO_RETRIEVE_VOICEMAIL_FOLDER
msgid "We are temporarily unable to display voice mail records."
msgstr "Chúng tôi tạm thời không thể hiển thị các bản ghi thư thoại."

#: voice.UNABLE_TO_UPDATE_CALL_FEATURES
msgctxt "voice.UNABLE_TO_UPDATE_CALL_FEATURES"
msgid ""
"We are temporarily unable to forward calls. We apologize for this "
"inconvenience."
msgstr ""
"Chúng tôi tạm thời không thể chuyển tiếp cuộc gọi. Chúng tôi xin lỗi vì sự "
"bất tiện này."

#: voice.UNABLE_TO_MODIFY_CALL_FEATURES
msgctxt "voice.UNABLE_TO_MODIFY_CALL_FEATURES"
msgid ""
"We are temporarily unable to forward calls. We apologize for this "
"inconvenience."
msgstr ""
"Chúng tôi tạm thời không thể chuyển tiếp cuộc gọi. Chúng tôi xin lỗi vì sự "
"bất tiện này."

#: voice.UNABLE_TO_MODIFY_VOICEMAIL_PREFS
msgid ""
"An error occurred while attempting to update your voice mail preferences. "
"Please try again."
msgstr ""
"Một lỗi xảy ra trong khi cố gắng cập nhật tùy chọn thư thoại của bạn. Hãy "
"thử lại."

#: voice.UNABLE_TO_FORWARD_VOICEMAIL
msgid "Your email could not be forwarded at this time. Please try again later."
msgstr ""
"Điện thư của bạn không thể được chuyển tiếp tại thời điểm này. Hãy thử lại "
"sau."

#: voice.UNABLE_TO_RETRIEVE_CALL_FEATURES
msgid "Unable to retrieve call features."
msgstr "Không thể nhận các đặc tính cuộc gọi."

#: voice.UNABLE_TO_RETRIEVE_VOICEMAIL_CONTENT
msgid "Unable to retrieve voice mail content."
msgstr "Không thể nhận nội dung thư thoại."

#: voice.UNABLE_TO_RETRIEVE_VOICEMAIL_PREFS
msgid "Unable to retrieve voice mail preferences."
msgstr "Không thể nhận tùy chọn thư thoại."

#: voice.UNABLE_TO_RETRIEVE_VOICEMAIL_SUMMARY
msgid "Unable to retrieve voice mail summary."
msgstr "Không thể nhận tóm lược của thư thoại."

#: voice.UNABLE_TO_MODIFY_VOICEMAIL_STATUS
msgid "Unable to modify voice mail status."
msgstr "Không thể thay đổi trạng thái thư thoại."

#: voice.UNABLE_TO_EMPTY_VOICEMAIL_TRASH
msgid "Unable to empty voice mail trash."
msgstr "Không thể làm trống thùng rác thư thoại"

#: voice.UNABLE_TO_DELETE_VOICEMAIL
msgid "Unable to delete voice mail."
msgstr "Không thể xóa thư thoại"

#: voice.UNABLE_TO_UNDELETE_VOICEMAIL
msgid "Unable to undelete voice mail."
msgstr "Không thể bỏ xóa thư thoại"
